#include <stdio.h>

int main() {
    float precoProduto, novoPreco;

    // Entrada do preço do produto
    printf("Digite o preco do produto: ");
    scanf("%f", &precoProduto);

    // Cálculo do novo preço com desconto de 9%
    novoPreco = precoProduto - (precoProduto * 0.09);

    // Saída do novo preço com desconto
    printf("O novo preco com desconto de 9 por cento: %.2f\n", novoPreco);

    return 0;
}
