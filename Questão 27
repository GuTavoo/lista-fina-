#include <stdio.h>

int main() {
    int numero;

    do {
        printf("Digite um n√∫mero: ");
        scanf("%d", &numero);
    } while (numero <= 0);

    printf("Tabuada do %d:\n", numero);
    for (int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }

    return 0;
}
